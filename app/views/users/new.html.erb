

<h1>NEW USER</h1>

<%= form_for [@user] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br/>
  <%= f.label :location_id %>
  <%= f.text_field :location_id %>
  <br/>
  <%= f.label :visit_id %>
  <%= f.text_field :visit_id %>
  <br/>
  <%= f.label :premium %>
  <%= f.text_field :premium %>

  <%= f.submit 'Create user' %>
  
<% end %>
